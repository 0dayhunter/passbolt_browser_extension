<div class="page login-form master-password ">
    <div class="input text required">
        <label for="UserUsername">Username</label>
        <input name="data[User][username]" class="required fluid" maxlength="50" type="text" id="UserUsername"
               required="required"
               value="" disabled="disabled"/>
    </div>
    <div class="input text required">
        <label for="js_master_password">Master password</label>
        <input type="password" placeholder="password" id="js_master_password" maxlength="50">
        <span class="security-token"></span>
        <div id='loginMessage' class="message helptext">Please enter your secret key password</div>
    </div>
    <div class="submit-wrapper clearfix">
        <a id='loginSubmit' class="button primary big">login</a>
    </div>
</div>