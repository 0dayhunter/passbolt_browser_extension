<!-- left column -->
<div class="col7">
    <h3>Information for public and secret key</h3>
    <table class="table-info">
        <tr>
            <td>Owner Name</td>
            <td  class="<%= fieldsDetails['name'] ? fieldsDetails['name'].status : '' %>"><%= keyInfo.userIds[0].name %> <span class="alt side">original name</span></td>
        </tr>
        <tr>
            <td>Owner Email</td>
            <td class="<%= fieldsDetails['email'] ? fieldsDetails['email'].status : '' %>"><%= keyInfo.userIds[0].email %></td>
        </tr>
        <tr>
            <td>Key Id</td>
            <td><%= keyInfo.keyId %></td>
        </tr>
        <tr>
            <td>Fingerprint</td>
            <td><%= keyInfo.fingerprint %></td>
        </tr>
        <tr>
            <td>Created</td>
            <td><%= keyInfo.created %></td>
        </tr>
        <tr>
            <td>Expires</td>
            <td><%= keyInfo.expires %></td>
        </tr>
        <tr>
            <td>Key Length</td>
            <td><%= keyInfo.length %></td>
        </tr>
        <tr>
            <td>Algorithm</td>
            <td><%= keyInfo.algorithm %></td>
        </tr>
    </table>
</div>

<!-- right column -->
<div class="col5 last">
    <div class="message <%= status %> side-message">
        <div>
            <label>Status</label>
            <span><%= status %></span>
        </div>
        <ul>
            <% for (var fieldName in fieldsDetails) { %>
            <li>
                <%= fieldName %> : <%= fieldsDetails[fieldName].rule %> (<%= fieldsDetails[fieldName].status %>)
            </li>
            <% } %>
        </ul>
        <!--<strong>Success!</strong>
        Nice one, it looks like a valid key with your key information matching the name and email provided by your passbolt administrator.
        <br><br>
        You are good to go!-->
    </div>
</div>